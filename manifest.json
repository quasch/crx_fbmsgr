{
   "name": "Facebook Chat",
   "description": "Este chat te permite ver tus mensajes de Facebook y contestarlos sin necesidad de entrar al sitio.",
   "version": "0.1",
   "manifest_version": 2,
   "icons": {
      "16": "icon_16.png",
      "48": "icon_48.png",
      "128": "icon_128.png"
   },
   "background": {
      "scripts": ["background.js"]
   },
   "browser_action": {
      "default_icon": {          
         "19": "icon_19.png",         
         "38": "icon_38.png"   
      },
      "default_title": "Facebook Chat"  
   }
}